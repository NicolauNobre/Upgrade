    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/UpGrade.jpg",
        "backgroundColor": "#1E1E1E"
      }
    },
    
    "web": {
      "favicon": "./src/assets/UpGrade.jpg"
    }
  }
}
