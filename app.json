{
  "expo": {
    "name": "UP",
    "slug": "UP",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/UPGrade.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/UPGrade.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1E"
